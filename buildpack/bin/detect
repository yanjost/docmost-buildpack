#!/usr/bin/env bash
set -e
source "$(dirname "$0")/../lib/common.sh"


if [[ -n "$DOCMOST_VERSION" ]]; then
  echo "docmost"
  exit 0
fi

if [[ -f "$BUILD_DIR/Procfile" ]]; then
  echo "docmost"
  exit 0
fi

echo ":: detect :: DOCMOST_VERSION not set and no Procfile found. Not a Docmost app."
exit 1
